{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@next-public-api-url"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next-public-api-url"
  },
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "buildCommand": "cd ../.. && pnpm --filter @peacase/web build",
  "outputDirectory": "apps/web/.next"
}
